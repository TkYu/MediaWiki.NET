{
  "version": "1.0.0-*",
  "title": "MediaWiki API .NET",
  "description": "This is a MediaWiki API .NET Wrapper",
  "authors": [ "TsungKang" ],
  "packOptions": {
    "owners": [ "TkYu" ],
    "repository": {
      "licenseUrl": "https://github.com/TkYu/MediaWiki.NET/blob/master/LICENSE",
      "projectUrl": "https://github.com/TkYu/MediaWiki.NET/",
      "type": "git",
      "url": "git://github.com/TkYu/MediaWiki.NET"
    },
    "tags": [
      "mediawiki",
      "wikipedia"
    ]
  },

  "dependencies": {
    "Newtonsoft.Json": "9.0.1"
  },

  "frameworks": {
    "netstandard1.6": {
      "imports": "dnxcore50",
      "dependencies": {
        "NETStandard.Library": "1.6.0",
        "System.Text.Encoding.CodePages": "4.0.1-*"
      }
    },
    "netstandard1.3": {
      "dependencies": {
        "System.Text.Encoding": "4.0.11-*",
        "System.Text.Encoding.CodePages": "4.0.1-*",
        "System.Runtime": "4.1.0.*",
        "System.Runtime.InteropServices": "4.1.0.*",
        "System.Net.Http": "4.1.0.*",
        "System.IO.FileSystem": "4.0.1.*"
      }
    },
    "net45": {
      "dependencies": {
        "System.Net.Http": "4.0.0.*"
      }
    },
    "net40": {},
    "net20": {}
  },
  "buildOptions": {
    "keyFile": "./key.snk",
    "xmlDoc": true
  },
  "scripts": {
    "postcompile": [
      "dotnet pack --no-build --configuration %compile:Configuration%"
    ]
  }
}